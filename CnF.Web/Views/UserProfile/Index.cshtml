@model IEnumerable<CnF.Domain.ViewModels.UserProfileViewModel>

@{
    ViewBag.Title = "Index";
} 

<h2>User Profile Index</h2>

<p>
    @Html.ActionLink("Create New", "Create")
</p>
<table class="table">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.UserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.BranchId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.FirstName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.LastName)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.MobileNo)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.IsActive)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.ResUserId)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SystemDate)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.SetDate)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.UserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.BranchId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.FirstName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.LastName)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.MobileNo)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.IsActive)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.ResUserId)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SystemDate)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.SetDate)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.UserId  }) |
           
        </td>
    </tr>
}

</table>
